{
  "name": "oxswap-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/oxswap/oxswap-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-local": "graph create oxswap/oxswap --node http://api.oxswap.one:8020/",
    "remove-local": "graph remove oxswap/oxswap --node http://graph-node.oxswap.one:8020/",
    "deploy-local": "graph deploy oxswap/oxswap --debug --ipfs http://api.oxswap.one:5001/ --node http://api.oxswap.one:8020/",
    "deploy": "graph deploy oxswap/oxswap --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-staging": "graph deploy $THE_GRAPH_GITHUB_USER/$THE_GRAPH_SUBGRAPH_NAME /oxswap --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/",
    "watch-local": "graph deploy oxswap/oxswap --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "web3": "^1.3.6",
    "yarn": "^1.22.10"
  }
}
